{
	"extendsConfigPath": "",
	"defineCombinations": [],
	"defaultSeverity": "INFO",
	"baseDefines": [],
	"version": 1,
	"exclude": {},
	"checks": [
		{
			"props": {},
			"type": "Anonymous"
		},
		{
			"props": {
				"allowSingleArgParens": true,
				"allowReturn": true,
				"allowFunction": true,
				"allowCurlyBody": true
			},
			"type": "ArrowFunction"
		},
		{
			"props": {
				"severity": "INFO"
			},
			"type": "AvoidStarImport"
		},
		{
			"props": {},
			"type": "BlockBreakingConditional"
		},
		{
			"props": {
				"thresholdSimilar": 120,
				"severityIdentical": "WARNING",
				"thresholdIdentical": 60
			},
			"type": "CodeSimilarity"
		},
		{
			"props": {
				"severity": "INFO"
			},
			"type": "CommentedOutCode"
		},
		{
			"props": {
				"policy": "aligned",
				"allowSingleline": true
			},
			"type": "ConditionalCompilation"
		},
		{
			"props": {
				"ignoreExtern": false,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": ["NOTINLINE"]
			},
			"type": "ConstantName"
		},
		{
			"props": {
				"thresholds": [
					{
						"complexity": 16,
						"severity": "WARNING"
					},
					{
						"complexity": 26,
						"severity": "ERROR"
					}
				],
				"severity": "IGNORE"
			},
			"type": "CyclomaticComplexity"
		},
		{
			"props": {
				"lineStyle": "ignore",
				"startStyle": "ignore",
				"severity": "IGNORE"
			},
			"type": "DocCommentStyle"
		},
		{
			"props": {
				"severity": "IGNORE"
			},
			"type": "Dynamic"
		},
		{
			"props": {
				"requireEmptyLineAfterPackage": false,
				"requireEmptyLineAfterInterface": false,
				"requireEmptyLineAfterAbstract": false,
				"allowEmptyLineAfterSingleLineComment": true,
				"max": 1,
				"requireEmptyLineAfterClass": false,
				"allowEmptyLineAfterMultiLineComment": true
			},
			"type": "EmptyLines"
		},
		{
			"props": {
				"enforceEmptyPackage": true
			},
			"type": "EmptyPackage"
		},
		{
			"props": {
				"none": [
					"beforePackage",
					"betweenImports",
					"beforeUsing",
					"typeDefinition",
					"afterLeftCurly",
					"beforeRightCurly",
					"betweenInterfaceFields"
				],
				"upto": [],
				"defaultPolicy": "ignore",
				"exact": [],
				"max": 1,
				"ignore": [],
				"atleast": [],
				"severity": "IGNORE",
				"skipSingleLineTypes": false
			},
			"type": "ExtendedEmptyLines"
		},
		{
			"props": {
				"requireParams": true,
				"fieldType": "BOTH",
				"requireReturn": true,
				"ignoreOverride": true,
				"tokens": [
					"ABSTRACT_DEF",
					"CLASS_DEF",
					"ENUM_DEF",
					"INTERFACE_DEF",
					"TYPEDEF_DEF"
				],
				"modifier": "PUBLIC",
				"excludeNames": ["new", "toString"]
			},
			"type": "FieldDocComment"
		},
		{
			"props": {
				"severity": "IGNORE"
			},
			"type": "Final"
		},
		{
			"props": {
				"character": "tab",
				"wrapPolicy": "larger",
				"ignoreComments": false,
				"ignoreConditionals": false,
				"conditionalPolicy": "aligned",
				"severity": "IGNORE"
			},
			"type": "Indentation"
		},
		{
			"props": {
				"character": "tab",
				"ignorePattern": "^$"
			},
			"type": "IndentationCharacter"
		},
		{
			"props": {
				"ignoreReturnAssignments": true,
				"severity": "IGNORE"
			},
			"type": "InnerAssignment"
		},
		{
			"props": {
				"ignoreSingleline": true,
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"ignoreEmptySingleline": true,
				"option": "eol"
			},
			"type": "LeftCurly"
		},
		{
			"props": {
				"ignoreSingleline": true,
				"tokens": ["TYPEDEF_DEF"],
				"ignoreEmptySingleline": true,
				"option": "eol"
			},
			"type": "LeftCurly"
		},
		{
			"props": {
				"max": 999999,
				"ignorePattern": "^$"
			},
			"type": "LineLength"
		},
		{
			"props": {
				"max": 999999,
				"ignoreEmptyLines": false
			},
			"type": "MethodLength"
		},
		{
			"props": {
				"max": 999999
			},
			"type": "NestedControlFlow"
		},
		{
			"props": {
				"max": 999999
			},
			"type": "NestedForDepth"
		},
		{
			"props": {
				"max": 999999
			},
			"type": "NestedIfDepth"
		},
		{
			"props": {
				"option": "questionMark"
			},
			"type": "NullableParameter"
		},
		{
			"props": {
				"newFunctionTypePolicy": "around",
				"ternaryOpPolicy": "around",
				"unaryOpPolicy": "ignore",
				"oldFunctionTypePolicy": "none",
				"boolOpPolicy": "around",
				"intervalOpPolicy": "none",
				"assignOpPolicy": "around",
				"bitwiseOpPolicy": "around",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"arrowPolicy": "around",
				"arrowFunctionPolicy": "around"
			},
			"type": "OperatorWhitespace"
		},
		{
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"...",
					"=>",
					"++",
					"--"
				],
				"option": "eol"
			},
			"type": "OperatorWrap"
		},
		{
			"props": {
				"prohibitMeta": false
			},
			"type": "RedundantAccessMeta"
		},
		{
			"props": {
				"prohibitMeta": false
			},
			"type": "RedundantAllowMeta"
		},
		{
			"props": {
				"enforcePrivate": false,
				"enforcePublic": false,
				"enforcePublicPrivate": false
			},
			"type": "RedundantModifier"
		},
		{
			"props": {
				"enforceReturnTypeForAnonymous": false,
				"allowEmptyReturn": true,
				"enforceReturnType": true
			},
			"type": "Return"
		},
		{
			"props": {
				"ignoreFormat": "^$",
				"max": 999999
			},
			"type": "ReturnCount"
		},
		{
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "aloneorsingle",
				"severity": "IGNORE"
			},
			"type": "RightCurly"
		},
		{
			"props": {
				"tokens": ["IF"],
				"option": "aloneorsingle",
				"severity": "IGNORE"
			},
			"type": "RightCurly"
		},
		{
			"props": {
				"tokens": ["OBJECT_DECL"],
				"option": "same",
				"severity": "IGNORE"
			},
			"type": "RightCurly"
		},
		{
			"props": {
				"commaPolicy": "after",
				"semicolonPolicy": "after",
				"allowTrailingComma": false,
				"dotPolicy": "none"
			},
			"type": "SeparatorWhitespace"
		},
		{
			"props": {
				"tokens": [","],
				"option": "eol"
			},
			"type": "SeparatorWrap"
		},
		{
			"props": {
				"spaceIfCondition": "should",
				"spaceAroundBinop": true,
				"spaceForLoop": "should",
				"ignoreRangeOperator": true,
				"spaceWhileLoop": "should",
				"spaceCatch": "should_not",
				"spaceSwitchCase": "should",
				"noSpaceAroundUnop": false
			},
			"type": "Spacing"
		},
		{
			"props": {
				"policy": "onlySingle",
				"allowException": false
			},
			"type": "StringLiteral"
		},
		{
			"props": {
				"format": "^\\s*(TODO|FIXME|HACK|XXX|BUG|MAYBE)"
			},
			"type": "TODOComment"
		},
		{
			"props": {
				"severity": "IGNORE"
			},
			"type": "Trace"
		},
		{
			"props": {},
			"type": "TrailingWhitespace"
		},
		{
			"props": {
				"ignoreEnumAbstractValues": true
			},
			"type": "Type"
		},
		{
			"props": {
				"tokens": [
					"ABSTRACT_DEF",
					"CLASS_DEF",
					"ENUM_DEF",
					"INTERFACE_DEF",
					"TYPEDEF_DEF"
				],
				"severity": "IGNORE"
			},
			"type": "TypeDocComment"
		},
		{
			"props": {},
			"type": "UnnecessaryConstructor"
		},
		{
			"props": {
				"moduleTypeMap": {},
				"ignoreModules": []
			},
			"type": "UnusedImport"
		},
		{
			"props": {},
			"type": "UnusedLocalVar"
		}
	],
	"numberOfCheckerThreads": 5
}
